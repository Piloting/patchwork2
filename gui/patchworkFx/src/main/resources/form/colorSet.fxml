<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane fx:id="mainPane" prefHeight="533.0" prefWidth="711.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.pilot.patchwork.controller.ColorSetController">
   <bottom>
      <FlowPane alignment="CENTER_RIGHT" hgap="10.0" prefHeight="53.0" prefWidth="600.0" vgap="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#cancelButtonPress" text="Отмена" />
            <Button mnemonicParsing="false" onAction="#okButtonPress" text="Ок" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </bottom>
   <center>
      <ScrollPane fx:id="vertScrollPane" hbarPolicy="NEVER" prefHeight="511.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="vBox" prefWidth="700.0" spacing="10.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </content></ScrollPane>
   </center>
   <top>
      <FlowPane hgap="10.0" prefHeight="53.0" prefWidth="600.0" vgap="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#addButtonPress" text="Добавить" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </top>
</BorderPane>
